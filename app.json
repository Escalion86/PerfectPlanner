{
  "expo": {
    "name": "PerfectPlanner",
    "slug": "PerfectPlanner",
    "version": "0.1.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "platforms": ["android"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "icon": "./assets/ic_stat_ac_unit.png",
      "useNextNotificationsApi": true,
      "package": "com.escalion.InterDB"
    }
  }
}
